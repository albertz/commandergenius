msgid "!SITTING_DUCKS!"
msgstr "!OTURAN_ÖRDEKLER!"

#: client/manager.c:246 client/manager.c:446
msgid "!HUNTER!"
msgstr "!AVCI!"

#: client/manager.c:247 client/manager.c:449
msgid "!INVADERS!"
msgstr "!İŞGALCİLER!"

